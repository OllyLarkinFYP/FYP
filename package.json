{
  "name": "electron-webpack-react-fable",
  "version": "1.0.0",
  "description": "A minimal electron-webpack framework using React TypeScript and F# Fable for core application logic",
  "scripts": {
    "start": "dotnet fable watch src/renderer/core-logic --run npm run dev",
    "compilefs": "dotnet fable src/renderer/core-logic",
    "build": "npm run compilefs && electron-webpack",
    "package": "npm run build && electron-builder",
    "postinstall": "dotnet tool restore && dotnet paket restore && npm run compilefs",
    "dev": "electron-webpack dev",
    "clean": "rimraf dist/ src/**/*.fs.js src/**/.fable/ src/**/bin/ src/**/obj/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OllyLarkinFYP/electron-webpack-react-fable.git"
  },
  "author": "Olly Larkin",
  "bugs": {
    "url": "https://github.com/OllyLarkinFYP/electron-webpack-react-fable/issues"
  },
  "homepage": "https://github.com/OllyLarkinFYP/electron-webpack-react-fable#readme",
  "electronWebpack": {
    "title": true,
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer"
    },
    "commonSourceDirectory": "src/common",
    "staticSourceDirectory": "src/static"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "webpack": "^4.42.1"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-support": "^0.5.19"
  }
}
